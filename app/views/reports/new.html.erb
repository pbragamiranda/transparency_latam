<h1>Reports#new</h1>

<div class="container">
	<%= simple_form_for @report do |f| %>
		<%= f.input :original_title %>
		<%= f.input :title_en %>
		<%= f.input :title_es %>
		<%= f.input :title_pt %>
		<%= f.input :year %>
		<%= f.input :theme %>
		<%= f.input :original_link %>

		<%= f.submit "Create Report", class: "btn btn-primary" %>
	<% end %>	
</div>
